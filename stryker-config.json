{
    "stryker-config":
    {
        "reporters": [
            "progress",
            "html"
        ],
        "log-level": "info",
        "log-file":true,
        "timeout-ms": 10000,
        "max-concurrent-test-runners": 4,
        "threshold-high": 80,
        "threshold-low": 70,
        "threshold-break": 60,
        "mutate": [
            "!Startup.cs"
        ],
        "ignore-methods": [
            "*Log*",
            "ToString",
            "*HashCode*"
        ],
        "dashboard-version": 1,
        "coverage-analysis": "perTest",
        "mutate-level": "Complete"
    }
}